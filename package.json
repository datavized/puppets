    "babel-preset-es2015": "^6.13.2",
    "compression-webpack-plugin": "^0.3.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.0",
    "del": "^2.0.2",
    "electricity": "^1.7.0",
