  "description": "Virtual Puppet Studio",
  "scripts": {
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "gulp dev",
    "watch": "gulp watch",
    "clean": "gulp clean",
