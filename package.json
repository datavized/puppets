  "jshintConfig": {
    "globals": {
      "console": true
    },
    "bitwise": true,
    "browser": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "esversion": 6,
    "forin": true,
    "freeze": true,
    "funcscope": true,
    "strict": "global",
    "immed": true,
    "iterator": true,
    "latedef": true,
    "maxparams": 5,
    "newcap": true,
    "noarg": true,
    "nonbsp": true,
    "nonew": true,
    "notypeof": true,
